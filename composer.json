{
  "name": "drdynamic/plox",
  "type": "project",
  "description": "PHP implementation of a lox interpreter",
  "keywords": [
    "lox",
    "interpreter"
  ],

  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Lox\\": "lox/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },

  "require": {
    "laravel/framework": "^10.39"
  },
  "require-dev": {
    "pestphp/pest": "^2.30"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
